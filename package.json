    "@sailthru/stui-regions": "2.25.1",
    "core-js": "3.21.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-is": "16.13.1",
    "react-router-dom": "6.3.0",
    "regenerator-runtime": "0.13.9",
    "styled-components": "5.3.3"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@babel/preset-env": "7.16.7",
    "@babel/preset-react": "7.16.7",
