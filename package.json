{
  "name": "hosted-pages-frontend",
  "engines": {
    "node": "^16.10.0",
    "npm": "^8.0.0"
  },
  "config": {
    "dev_server_port": "9088"
  },
  "scripts": {
    "dev": "npm run serve",
    "serve": "webpack serve -c webpack/webpack.devServer.js",
    "build": "webpack -c webpack/webpack.build.js",
    "lint": "eslint src/{,*/**/}*.{js,jsx}",
